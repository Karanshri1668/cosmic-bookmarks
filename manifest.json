{
  "manifest_version": 3,
  "name": "My Bookmarks Dashboard",
  "version": "1.0",
  "description": "A beautiful tile-based display of all bookmarks.",
  "permissions": ["bookmarks"],
  "action": {
  "default_icon": {
    "16": "icons/icon48.png",
    "48": "icons/icon48.png"
  },
  "default_popup": "dashboard.html"
},
"icons": {
  "16": "icons/icon48.png",
  "48": "icons/icon48.png",
  "128": "icons/icon48.png"
},
  "host_permissions": ["<all_urls>"]
}


